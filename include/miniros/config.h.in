#cmakedefine HAVE_TRUNC
#cmakedefine HAVE_IFADDRS_H
#cmakedefine HAVE_EPOLL
#cmakedefine MINIROS_BUILD_SHARED_LIBS

#define MINIROS_VERSION_MAJOR @miniros_VERSION_MAJOR@
#define MINIROS_VERSION_MINOR @miniros_VERSION_MINOR@
#define MINIROS_VERSION_PATCH @miniros_VERSION_PATCH@
#define MINIROS_VERSION_COMBINED(major, minor, patch) (((major) << 20) | ((minor) << 10) | (patch))
#define MINIROS_VERSION ROS_VERSION_COMBINED(MINIROS_VERSION_MAJOR, MINIROS_VERSION_MINOR, MINIROS_VERSION_PATCH)
#define MINIROS_VERSION_GE(major1, minor1, patch1, major2, minor2, patch2) (MINIROS_VERSION_COMBINED(major1, minor1, patch1) >= MINIROS_VERSION_COMBINED(major2, minor2, patch2))
#define MINIROS_VERSION_MINIMUM(major, minor, patch) MINIROS_VERSION_GE(MINIROS_VERSION_MAJOR, MINIROS_VERSION_MINOR, MINIROS_VERSION_PATCH, major, minor, patch)
